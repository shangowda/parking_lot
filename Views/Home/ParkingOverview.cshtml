@model IEnumerable<parking_lot.Models.ParkingOverview>
@{
    ViewData["Title"] = "ParkingOverview";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h1>Parking Lot Overview</h1>
<div class="row">
    <div class="col-md-12 table">
        <table>
            <thead>
                <tr>
                    <th>Lot Name</th>
                    <th>Space Type</th>
                    <th>Total Space</th>
                    <th>Space Taken</th>
                    <th>Space Open</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {

                    <tr>
                        <td>@item.LotName</td>
                        <td>@item.SpaceType</td>
                        <td>@item.Total</td>
                        <td>@item.Taken</td>
                        <td>@item.Open</td>
                    </tr>

                }

            </tbody>
        </table>
    </div>
</div>

